{
  "extends": "astro/tsconfigs/base",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["src/assets/*"]
    },
    "allowJs": true,
    "checkJs": false,
    "noImplicitAny": false,
    "strict": false,
    "strictNullChecks": false,
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
